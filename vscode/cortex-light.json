{
  "type": "light",
  "displayName": "Cortex Light",
  "name": "cortex-light",
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#2D2D2D"
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#787C99"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.other.enummember",
        "variable.language"
      ],
      "settings": {
        "foreground": "#0066CC"
      }
    },
    {
      "scope": ["entity", "entity.name"],
      "settings": {
        "foreground": "#7C3AED"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#2D2D2D"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#16A34A"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#D6336C"
      }
    },
    {
      "scope": ["storage", "storage.type"],
      "settings": {
        "foreground": "#D6336C"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#2D2D2D"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#2E7D32"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#0066CC"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#0066CC"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#B45309"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#2D2D2D"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#DC2626"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#DC2626"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#DC2626"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#DC2626"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "background": "#D6336C",
        "content": "^M",
        "fontStyle": "italic underline",
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#DC2626"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#0066CC"
      }
    },
    {
      "scope": ["source.regexp", "string.regexp"],
      "settings": {
        "foreground": "#2E7D32"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#2E7D32"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "foreground": "#16A34A"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#0066CC"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#0066CC"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#0066CC"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#B45309"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "foreground": "#0066CC"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#16A34A"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2D2D2D"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2D2D2D"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.strikethrough"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#0066CC"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "background": "#FEE2E2",
        "foreground": "#DC2626"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "background": "#DCFCE7",
        "foreground": "#16A34A"
      }
    },
    {
      "scope": ["markup.changed", "punctuation.definition.changed"],
      "settings": {
        "background": "#FEF3C7",
        "foreground": "#B45309"
      }
    },
    {
      "scope": ["markup.ignored", "markup.untracked"],
      "settings": {
        "background": "#0066CC",
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#7C3AED"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#0066CC"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "foreground": "#0066CC"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#0066CC"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#4B5563"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#DC2626"
      }
    },
    {
      "scope": ["constant.other.reference.link", "string.other.link"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#2E7D32"
      }
    }
  ]
}
