{
  "displayName": "Cortex Dark",
  "name": "cortex-dark",
  "colors": {
    "editor.background": "#101017",
    "editor.foreground": "#E8EAF2"
  },
  "semanticHighlighting": true,
  "settings": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#7F8499"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.other.enummember",
        "variable.language"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": ["entity", "entity.name"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#E8EAF2"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#7AE0A7"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#FF7AB2"
      }
    },
    {
      "scope": ["storage", "storage.type"],
      "settings": {
        "foreground": "#FF7AB2"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#E8EAF2"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#E8EAF2"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF8A9E"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF8A9E"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF8A9E"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF8A9E"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "background": "#FF7AB2",
        "content": "^M",
        "fontStyle": "italic underline",
        "foreground": "#1A1D24"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#FF8A9E"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": ["source.regexp", "string.regexp"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "foreground": "#7AE0A7"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#7AE0A7"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E8EAF2"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#E8EAF2"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.strikethrough"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "background": "#86181D",
        "foreground": "#FF8A9E"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "background": "#144620",
        "foreground": "#7AE0A7"
      }
    },
    {
      "scope": ["markup.changed", "punctuation.definition.changed"],
      "settings": {
        "background": "#C24E00",
        "foreground": "#FFB86C"
      }
    },
    {
      "scope": ["markup.ignored", "markup.untracked"],
      "settings": {
        "background": "#82AAFF",
        "foreground": "#1A1D24"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#A9B1D6"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#FF8A9E"
      }
    },
    {
      "scope": ["constant.other.reference.link", "string.other.link"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#C3E88D"
      }
    }
  ]
}
